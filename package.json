{
    "dependencies": {
        "concurrently": "^9.1.0"
    },
    "scripts": {
        "start": "concurrently --prefix \"[{name}]\" --names \"frontend,backend\" \"npm run start:frontend\" \"npm run start:backend\"",
        "start:frontend": "cd frontend && npm run dev",
        "start:backend": "cd backend && npm run dev"
    }
}
